<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="289.9dp"
    android:layout_height="435.8dp"
    android:orientation="vertical"
    android:layout_gravity="center_horizontal"
    android:background="#ffffff">

    <de.hdodenhof.circleimageview.CircleImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/profile_image"
        android:src="@drawable/vlu_logo"
        android:layout_width="165.1dp"
        android:layout_height="165.1dp"
        android:layout_marginTop="46.1dp"
        android:layout_gravity="center"
        app:civ_border_color="#FF000000"/>
    <com.example.admin.flerken.customfonts.MyTextView_Roboto_Regular
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nguyễn Hữu Kha"
        android:textColor="#111111"
        android:layout_gravity="center"
        android:textSize="12.5sp"
        android:layout_marginTop="25sp"/>

    <com.example.admin.flerken.customfonts.MyTextView_Roboto_Regular
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Khoa: Kĩ thuật phần mềm"
        android:textColor="#111111"
        android:layout_gravity="center"
        android:textSize="12.5sp"
        android:layout_marginTop="5sp"/>

    <com.example.admin.flerken.customfonts.MyTextView_Roboto_Regular
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Số điện thoại: 0919555666"
        android:textColor="#111111"
        android:layout_gravity="center"
        android:textSize="12.5sp"
        android:layout_marginTop="5sp"/>
    <com.example.admin.flerken.customfonts.Button_Roboto_Medium
        android:layout_width="120dp"
        android:layout_height="48.3dp"
        android:text="OK"
        android:paddingHorizontal="50dp"
        android:textSize="15dp"
        android:layout_gravity="center"
        android:textColor="#ffffff"
        android:layout_marginTop="45.1dp"
        android:background="@drawable/btnrectangle"
        android:id="@+id/ButtonOkContact"/>



</LinearLayout>